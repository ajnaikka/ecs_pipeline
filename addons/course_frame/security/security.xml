<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="module_category_course_frame" model="ir.module.category">
            <field name="name">Academy</field>
            <field name="description">Helps you manage your academy different-different users.</field>
            <field name="sequence">15</field>
        </record>

        <record id="group_academy_content_writer" model="res.groups">
            <field name="name">Content Writer</field>
            <field name="category_id" ref="module_category_course_frame"/>
            <field name="users" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record id="group_academy_team_lead" model="res.groups">
            <field name="name">Team Lead</field>
            <field name="category_id" ref="module_category_course_frame"/>
            <field name="users" eval="[(4, ref('base.group_user'))]"/>
<!--            <field name="implied_ids" eval="[(4, ref('course_frame.group_academy_content_writer'))]"/>-->
        </record>

        <record id="group_academy_academy_head" model="res.groups">
            <field name="name">Academy Head</field>
            <field name="category_id" ref="module_category_course_frame"/>
            <field name="users" eval="[(4, ref('base.group_user'))]"/>
<!--            <field name="implied_ids" eval="[(4, ref('course_frame.group_academy_team_lead'))]"/>-->
        </record>

        <record id="group_academy_associate_dean" model="res.groups">
            <field name="name">Associate Dean</field>
            <field name="category_id" ref="module_category_course_frame"/>
            <field name="users" eval="[(4, ref('base.group_user'))]"/>
<!--            <field name="implied_ids" eval="[(4, ref('course_frame.group_academy_academy_head'))]"/>-->
        </record>

        <record id="group_academy_director" model="res.groups">
            <field name="name">Director</field>
            <field name="category_id" ref="module_category_course_frame"/>
            <field name="users" eval="[(4, ref('base.group_user'))]"/>
<!--            <field name="implied_ids" eval="[(4, ref('course_frame.group_academy_associate_dean'))]"/>-->
        </record>

    </data>
</odoo>
