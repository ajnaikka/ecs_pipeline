<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="website_menu_quotations" model="openeducat.portal.menu">
        <field name="name">Quotations</field>
        <field name="link">/my/quotes</field>
        <field name="active">True</field>
        <field name="sequence" type="int">115</field>
        <field name="background_color">#45ac34</field>
        <field name="is_visible_to_parent">False</field>
        <field name="icon_image" type="base64"
               file="openeducat_online_admission/static/img/quotation.png"/>
    </record>

    <record id="website_menu_sales_orders" model="openeducat.portal.menu">
        <field name="name">Sales Orders</field>
        <field name="link">/my/orders</field>
        <field name="active">True</field>
        <field name="sequence" type="int">120</field>
        <field name="background_color">#304cd5</field>
        <field name="is_visible_to_parent">False</field>
        <field name="icon_image" type="base64"
               file="openeducat_online_admission/static/img/sale_order.png"/>
    </record>

</odoo>
