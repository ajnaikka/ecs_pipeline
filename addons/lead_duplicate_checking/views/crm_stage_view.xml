<odoo>
    <data>
        <record model="ir.ui.view" id="crm_stage_configuration_form">
            <field name="name">crm.stage.form</field>
            <field name="model">crm.stage</field>
            <field name="inherit_id" ref="crm.crm_stage_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='is_won']" position="after">
                    <field name="avoid_duplicate_check"/>
                    <field name="duplicate_stage"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>