<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <!-- Inherit Form view -->
  <record id="project_edit_form" model="ir.ui.view">
    <field name="name">project.project.form</field>
    <field name="model">project.project</field>
    <field name="inherit_id" ref="project.edit_project"/>
    <field name="arch" type="xml">
      <header position="inside">
        <button name="createCourse" string="Create Course" type="object"
          attrs="{'invisible':[('course_id','!=',False)]}"
          groups="asbm_main.group_asbm_admission_manager"/>
      </header>
      <field name="partner_id" position="after">
        <field name="course_id"/>
<!--        <field name="course_id" groups="base.group_no_one"/>-->
<!--        <field name="course_frame_id"/>-->
<!--        <field name="course_frame_id" groups='base.group_no_one'/>-->
      </field>
    </field>
  </record>
</odoo>
