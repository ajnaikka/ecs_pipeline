              parent="openeducat_core.menu_op_school_config_course"
              action="op_case_study_action_form" sequence="9"/>


    <record id="showing_as_type_form_view" model="ir.ui.view">
        <field name="name">showing.as</field>
        <field name="model">showing.as</field>
        <field name="arch" type="xml">
            <form string="Showing as">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>


                        </group>
                    </group>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids"/>
                    <field name="message_ids"/>
                </div>
            </form>
        </field>
    </record>
    <record id="showing_as_tree_view" model="ir.ui.view">
        <field name="name">showing.as.tree</field>
        <field name="model">showing.as</field>
        <field name="arch" type="xml">
            <tree string="Showing as">
                <field name="name" required="1"/>


            </tree>
        </field>
    </record>

    <record id="action_showing_as_details" model="ir.actions.act_window">
        <field name="name">Showing as</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">showing.as</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem action="action_showing_as_details" id="menu_showing_as"
              parent="openeducat_core.menu_op_school_config_course" name="Showing as" sequence="3"/>

    <record id="showing_in_type_form_view" model="ir.ui.view">
        <field name="name">showing.in</field>
        <field name="model">showing.in</field>
        <field name="arch" type="xml">
            <form string="Showing in">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>


                        </group>
                    </group>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids"/>
                    <field name="message_ids"/>
                </div>
            </form>
        </field>
    </record>
    <record id="showing_in_tree_view" model="ir.ui.view">
        <field name="name">showing.in.tree</field>
        <field name="model">showing.in</field>
        <field name="arch" type="xml">
            <tree string="Showing in">
                <field name="name" required="1"/>
            </tree>
        </field>
    </record>

    <record id="action_showing_in_details" model="ir.actions.act_window">
        <field name="name">Showing in</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">showing.in</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem action="action_showing_in_details" id="menu_showing_in"
              parent="openeducat_core.menu_op_school_config_course" name="Showing in" sequence="4"/>

