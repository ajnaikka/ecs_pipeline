    <!-- op.projects -->
    <record id="op_project_view_form" model="ir.ui.view">
        <field name="name">op.project.form</field>
        <field name="model">op.project</field>
        <field name="arch" type="xml">
            <form string="Op Project">
                <sheet>
                    <div class="oe_title">
                        <label for="name" class="oe_edit_only"/>
                        <h1>
                            <field name="name"/>
                        </h1>
                    </div>
                    <group col="4">
                        <field name="code"/>
                        <field name="marks_in"/>
                        <field name="showing_as"/>
                        <field name="showing_in"/>
                        <field name="position"/>
                        <field name="show_in_main_sheet"/>
                        <field name="show_in_mark_sheet"/>
                        <field name="show_in_consolidated"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="op_project_tree_view_tree" model="ir.ui.view">
        <field name="name">op.project.tree</field>
        <field name="model">op.project</field>
        <field name="arch" type="xml">
            <tree string="Op Project">
                <field name="name"/>
                <field name="code"/>
                <field name="marks_in"/>
                <field name="showing_as"/>
                <field name="showing_in"/>
                <field name="position"/>
                <field name="show_in_main_sheet"/>
                <field name="show_in_mark_sheet"/>
                <field name="show_in_consolidated"/>
            </tree>
        </field>
    </record>
    <record id="op_project_view_search" model="ir.ui.view">
        <field name="name">op.project.search</field>
        <field name="model">op.project</field>
        <field name="arch" type="xml">
            <search string="Op project search">
                <field name="name"/>
                <field name="code"/>
            </search>
        </field>
    </record>
    <record id="op_project_action_form" model="ir.actions.act_window">
        <field name="name">Projects</field>
        <field name="res_model">op.project</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
            </p>
            <p>
            </p>
        </field>
    </record>
    <menuitem id="menu_op_project_action" name="Projects"
              parent="openeducat_core.menu_op_school_config_course"
              action="op_project_action_form" sequence="9"/>

    <!-- op case study -->
    <record id="op_case_study_view_form" model="ir.ui.view">
