    <record id="op_case_study_view_search" model="ir.ui.view">
        <field name="name">op.case.study.search</field>
        <field name="model">op.case.study</field>
        <field name="arch" type="xml">
            <search string="Op project search">
                <field name="name"/>
                <field name="code"/>
            </search>
        </field>
    </record>
    <record id="op_case_study_action_form" model="ir.actions.act_window">
        <field name="name">Case Study</field>
        <field name="res_model">op.case.study</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
            </p>
            <p>
            </p>
        </field>
    </record>
