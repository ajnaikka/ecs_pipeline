                        <field name="position"/>
                        <field name="show_in_main_sheet"/>
                        <field name="show_in_mark_sheet"/>
                        <field name="show_in_consolidated"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="op_case_study_tree_view_tree" model="ir.ui.view">
        <field name="name">op.case.study.tree</field>
        <field name="model">op.case.study</field>
        <field name="arch" type="xml">
            <tree string="Op Project">
                <field name="name"/>
                <field name="code"/>
                <field name="marks_in"/>
                <field name="showing_as"/>
