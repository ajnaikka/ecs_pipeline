<odoo>
    <data>
        <record id="hide_enroll_slide_channel_form_view" model="ir.ui.view">
            <field name="name">slide.channel.hide.enroll</field>
            <field name="model">slide.channel</field>
            <field name="inherit_id" ref="website_slides.view_slide_channel_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='options']//group/field[@name='enroll']" position="after">
                    <field name="hide_enroll_course"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>