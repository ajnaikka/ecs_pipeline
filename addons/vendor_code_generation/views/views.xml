<record id="view_res_partner_vendor_code_tree" model="ir.ui.view">
        <field name="name">res.partner.vendor.code.inherit.tree</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_tree"/>
        <field name="arch" type="xml">
            <field name="display_name" position="after">
                     <field name="unique_reference_id" readonly="True" invisible="supplier_rank == False"/>
                 <field name="supplier_rank" invisible="1" optional="hide"/>
            </field>
        </field>
    </record>



       <record id="vendor_id_view_purchase_print_button" model="ir.ui.view">
            <field name="name">purchase.order.vendor.id</field>
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_form"/>
            <field name="arch" type="xml">
                <field name="partner_ref" position="after">
			        <field name="vendor_id" readonly="True" force_save="1"/>
			    </field>
            </field>
       </record>





