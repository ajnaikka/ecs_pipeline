<odoo>
    <data>
        <record id="res_partner_qualification" model="ir.ui.view">
            <field name="name">res.partner.last.qualification</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="openeducat_custom.openeducat_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='other']/field[@name='application_for_admission']" position="after">
                    <field name="last_qualification"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>