<odoo>
    <data>
        <record id="view_hr_employee_policy_inherit" model="ir.ui.view">
            <field name="name">hr.employee.policy.inherit</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='work_email']" position="after">
                    <field name="have_agreed_policy"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>